{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "azuyalabs/yasumi": "^2.5",
        "doctrine/annotations": "1.13.2",
        "doctrine/doctrine-bundle": "2.6.3",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.12.2",
        "dompdf/dompdf": "^2.0",
        "ekreative/health-check-bundle": "1.10",
        "fakerphp/faker": "1.19.0",
        "friendsofsymfony/rest-bundle": "^3.3",
        "fsevestre/boolean-form-type": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.8",
        "lexik/jwt-authentication-bundle": "^2.15",
        "nesbot/carbon": "2.58.0",
        "nexylan/slack-bundle": "2.3.0",
        "php-http/guzzle6-adapter": "2.0.2",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "phpstan/phpdoc-parser": "1.5.1",
        "sensio/framework-extra-bundle": "6.1.0",
        "stof/doctrine-extensions-bundle": "1.7.0",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "1.18.7",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/swiftmailer-bundle": "3.5.4",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/twilio-notifier": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/webpack-encore-bundle": "1.14.1",
        "symfony/yaml": "5.4.*",
        "twilio/sdk": "^6.43",
      "ext-openssl": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "9.5.20",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "1.43.0",
        "symfony/phpunit-bridge": "6.0.8",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "code-check": [
            "./vendor/bin/phpcs src/ --standard=phpcs.xml --extensions=php"
        ],
        "code-fix": [
            "php vendor/bin/php-cs-fixer fix src --rules='{\"@PSR2\": true, \"@Symfony\": true, \"concat_space\": {\"spacing\": \"one\"}}' -v --using-cache=no"
        ],
        "phpunit-test": [
            "php vendor/bin/phpunit"
        ],
        "post-install-cmd": [

        ],
        "post-update-cmd": [

        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
